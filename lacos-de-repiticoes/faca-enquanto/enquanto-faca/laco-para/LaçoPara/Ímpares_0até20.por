programa
{
	
	funcao inicio()
	{
		inteiro numero
		escreva("Os números ímpares entre 0 e 20 são:")

		para(numero = 0; numero<=20; numero++){

			se(numero %2 ==1)
			escreva(numero, "\n")
		}
	}
}
